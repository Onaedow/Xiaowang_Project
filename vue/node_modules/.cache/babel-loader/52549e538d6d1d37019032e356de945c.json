{"ast":null,"code":"import request from \"@/utils/request\";\nimport '@wangeditor/editor/dist/css/style.css';\nimport { createEditor, createToolbar, IEditorConfig, IDomEditor } from '@wangeditor/editor';\nlet editor;\nlet editorConfig;\nconst E = window.wangEditor; // 全局变量\n\nexport default {\n  name: 'News',\n  components: {},\n\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      vis: false,\n      detail: {}\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  mounted() {},\n\n  methods: {\n    details(row) {\n      this.detail = row;\n      this.vis = true;\n    },\n\n    filesUploadSuccess(res) {\n      console.log(res);\n      this.form.cover = res.data;\n    },\n\n    load() {\n      request.get(\"/news\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n      this.$nextTick(() => {\n        // 编辑器配置\n        const editorConfig = {};\n        editorConfig.placeholder = '请输入内容';\n\n        editorConfig.onChange = editor => {\n          // 当编辑器选区、内容变化时，即触发\n          console.log('content', editor.children);\n          console.log('html', editor.getHtml());\n        }; // 工具栏配置\n\n\n        const toolbarConfig = {}; //关联弹窗里面的div,new一个editor对象\n\n        editor = createEditor({\n          selector: '#editor-container',\n          html: '<p></p>',\n          // 从 editor.getHtml() 获取的 html 内容\n          config: editorConfig,\n          mode: 'default' // 或 'simple' 参考下文\n\n        });\n        const toolbar = createToolbar({\n          editor,\n          selector: '#toolbar-container',\n          config: toolbarConfig,\n          mode: 'default' // 或 'simple' 参考下文\n\n        });\n      });\n    },\n\n    save() {\n      this.form.content = editor.getHtml(); //获取编辑器里面的值，然后赋予到实体\n\n      if (this.form.id) {\n        //update\n        request.put(\"/news\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n\n          this.load(); //刷新表格的数据\n\n          this.dialogVisible = false; //关闭弹窗\n        });\n      } else {\n        //add a new one\n        let userStr = sessionStorage.getItem(\"user\") || \"{}\";\n        let user = JSON.parse(userStr);\n        this.form.author = user.nickname;\n        request.post(\"/news/save\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n\n          this.load(); //刷新表格的数据\n\n          this.dialogVisible = false; //关闭弹窗\n        });\n      }\n\n      request.post(\"/news\", this.form).then(res => {\n        console.log(res);\n      });\n    },\n\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"/news/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n      this.load();\n    },\n\n    handleSizeChange(pageSize) {\n      //改变每页个数触发\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      //改变页码触发\n      this.currentPage = pageNum;\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoGA,OAAOA,OAAP,MAAoB,iBAApB;AACA,OAAO,uCAAP;AACA,SAASC,YAAT,EAAuBC,aAAvB,EAAsCC,aAAtC,EAAqDC,UAArD,QAAuE,oBAAvE;AAEA,IAAIC,MAAJ;AACA,IAAIC,YAAJ;AACA,MAAMC,IAAIC,MAAM,CAACC,UAAjB,C,CAA6B;;AAE7B,eAAe;EACbC,IAAI,EAAE,MADO;EAEbC,UAAU,EAAE,EAFC;;EAIbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,EADD;MAELC,aAAa,EAAE,KAFV;MAGLC,MAAM,EAAE,EAHH;MAILC,WAAW,EAAE,CAJR;MAKLC,QAAQ,EAAE,EALL;MAMLC,KAAK,EAAE,CANF;MAOLC,SAAS,EAAE,EAPN;MAQLC,GAAG,EAAE,KARA;MASLC,MAAM,EAAE;IATH,CAAP;EAWD,CAhBY;;EAkBbC,OAAO,GAAG;IACR,KAAKC,IAAL;EACD,CApBY;;EAsBbC,OAAO,GAAG,CAET,CAxBY;;EA0BbC,OAAO,EAAE;IACPC,OAAO,CAACC,GAAD,EAAM;MACX,KAAKN,MAAL,GAAcM,GAAd;MACA,KAAKP,GAAL,GAAW,IAAX;IACD,CAJM;;IAMPQ,kBAAkB,CAACC,GAAD,EAAM;MACtBC,OAAO,CAACC,GAAR,CAAYF,GAAZ;MACA,KAAKhB,IAAL,CAAUmB,KAAV,GAAkBH,GAAG,CAACjB,IAAtB;IACD,CATM;;IAWPW,IAAI,GAAG;MACLvB,OAAO,CAACiC,GAAR,CAAY,OAAZ,EAAqB;QACnBC,MAAM,EAAE;UACNC,OAAO,EAAE,KAAKnB,WADR;UAENC,QAAQ,EAAE,KAAKA,QAFT;UAGNF,MAAM,EAAE,KAAKA;QAHP;MADW,CAArB,EAMGqB,IANH,CAMQP,GAAE,IAAK;QACbC,OAAO,CAACC,GAAR,CAAYF,GAAZ;QACA,KAAKV,SAAL,GAAiBU,GAAG,CAACjB,IAAJ,CAASyB,OAA1B;QACA,KAAKnB,KAAL,GAAaW,GAAG,CAACjB,IAAJ,CAASM,KAAtB;MACD,CAVD;IAWD,CAvBM;;IAyBPoB,GAAG,GAAG;MACJ,KAAKxB,aAAL,GAAqB,IAArB;MACA,KAAKD,IAAL,GAAY,EAAZ;MAEA,KAAK0B,SAAL,CAAe,MAAM;QACnB;QACA,MAAMjC,YAAW,GAAI,EAArB;QACAA,YAAY,CAACkC,WAAb,GAA2B,OAA3B;;QACAlC,YAAY,CAACmC,QAAb,GAAyBpC,MAAD,IAAY;UAClC;UACAyB,OAAO,CAACC,GAAR,CAAY,SAAZ,EAAuB1B,MAAM,CAACqC,QAA9B;UACAZ,OAAO,CAACC,GAAR,CAAY,MAAZ,EAAoB1B,MAAM,CAACsC,OAAP,EAApB;QACF,CAJA,CAJmB,CASnB;;;QACA,MAAMC,aAAY,GAAI,EAAtB,CAVmB,CAYnB;;QACAvC,MAAK,GAAIJ,YAAY,CAAC;UACpB4C,QAAQ,EAAE,mBADU;UAEpBC,IAAI,EAAE,SAFc;UAEH;UACjBC,MAAM,EAAEzC,YAHY;UAIpB0C,IAAI,EAAE,SAJc,CAIJ;;QAJI,CAAD,CAArB;QAMA,MAAMC,OAAM,GAAI/C,aAAa,CAAC;UAC5BG,